#include<stdio.h>
int main()
{
    int i;
    for(i=1;i<=5;i++)
    {
         if(i==1)
         {
             printf("1");
             printf("                 ");
             printf("1\n");
         }
         if(i==2)
         {
             printf("1 2");
             printf("             ");
             printf("2 1\n");
         }
         if(i==3)
         {
             printf("1 2 3");
             printf("         ");
             printf("3 2 1\n");
         }
         if(i==4)
         {
             printf("1 2 3 4     ");
             printf("4 3 2 1\n");
         }
         if(i==5)
         {
             printf("1 2 3 4 5 ");
             printf("5 4 3 2 1\n");
         }
    }
    return 0;
}
